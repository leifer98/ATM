.global _start

.section .text

  mov $seconddegree, %rax
  cmpl $1, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
series: .int -3, -2, 1, 6, 13, 22, 33, 46, 61, 78, 97, 118, 141

size: .int 13

seconddegree: .byte 0
