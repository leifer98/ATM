.global _start

.section .text

  mov $seconddegree, %rax
  cmpl $0, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
series: .int 2, -3, 6, -9, 18, -27, 54, -81, 162, -243, 486, -729
#           -1.5 -2-1.5 -2
size: .int 12

seconddegree: .byte 0
