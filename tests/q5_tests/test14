.global _start

.section .text

  mov $seconddegree, %rax
  cmpl $1, (%rax)
  jne bad_exit

  movq $60, %rax
  movq $0, %rdi
  syscall

bad_exit:
  movq $60, %rax
  movq $1, %rdi
  syscall

.section .data
series: .int 3, -6, 12, -24, 48, -96, 192, -384, 768, -1536, 3072, -6144

size: .int 12

seconddegree: .byte 0
